#-------------------------------------------------------------------
# Chargement des fichiers de configuration par défaut
#-------------------------------------------------------------------
if [ -f $MODCOD_FILE ] ; then
   echo Config des MODCODs depuis $MODCOD_FILE
   . $MODCOD_FILE
else
   echo ATTENTION, pas de MODCOD configure
   exit
fi

